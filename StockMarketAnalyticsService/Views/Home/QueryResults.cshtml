@model IEnumerable<FinVizDataService.Models.FinVizDataItem>
@{
    ViewBag.Title = "Query Execution Result";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h1>Query Execution Result</h1>
<table>
    <thead>
        <tr>
            <th>Ticker</th>
            <th>Date</th>
            <th>Properties</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Ticker</td>
                <td>@item.Date</td>
                <td>
                    @foreach (var kvp in item.ItemProperties)
                    {
                        <p>@kvp.Key: @kvp.Value</p>
                    }
                </td>
            </tr>
        }
    </tbody>
</table>
